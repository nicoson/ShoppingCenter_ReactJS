{
  "name": "reactproj",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "antd": "^2.13.0",
    "react": "^15.6.1",
    "react-bootstrap": "^0.31.2",
    "react-dom": "^15.6.1",
    "react-scripts": "1.0.12",
    "redux": "^3.7.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "react",
      "latest",
      "stage-0"
    ],
    "plugins": [
